<%- [@parameters].flatten.compact.each do |parameter| -%>
  <%- parameter.each_pair do |attrib, value| %>
    <%- if attrib == parameter.keys.first -%>
    <Parameter <%= attrib %>=<%= "#{value}".encode(:xml => :attr) -%>
    <%- else -%>
               <%= attrib %>=<%= "#{value}".encode(:xml => :attr) -%>
    <%- end -%>
  <%- end %> />
<%- end -%>
