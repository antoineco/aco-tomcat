<%- [@connectors].flatten.compact.each do |connector| %>
  <%- connector.each_pair do |attrib, value| -%>
    <%- if attrib == connector.keys.first -%>
    <Connector <%= attrib %>=<%= "#{value}".encode(:xml => :attr) %><% if attrib == connector.keys.last %> /><% end %>
    <%- else -%>
               <%= attrib %>=<%= "#{value}".encode(:xml => :attr) %><% if attrib == connector.keys.last %> /><% end %>
    <%- end -%>
  <%- end -%>
<%- end -%>
